---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

  <title>indspenceable</title>
  <link href="http://indspenceable.com/feed/" rel="self"/>
  <link href="http://indspenceable.com/"/>
  <updated>{{ site.time | date_to_xmlschema }}</updated>
  <id>http://indspenceable.com/</id>
  <author>
    <name>indspenceable</name>
    <email>daniel.patrick.spencer@gmail.com</email>
  </author>

  {% for post in site.posts %}
  <entry>
    <title>{{ post.title }}</title>
    <link href="http://indspenceable.com{{ post.url }}"/>
    <updated>{{ post.date | date_to_xmlschema }}</updated>
    <id>http://indspenceable.com{{ post.id }}</id>
    <content type="html">{{ post.content | xml_escape }}</content>
  </entry>
  {% endfor %}

</feed>
